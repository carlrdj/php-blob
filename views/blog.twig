{% extends "layout.twig" %}
{% block content %}
					<div class="blog-post">
						<h2>{{ blogPost.titulo }}</h2>
						<p>Jan 1, 2020 by <a href="">Carl</a></p>
						{% if blogPost.img_url %}
							<div class="blog-post-image">
								<img src="{{ blogPost.img_url }}" alt="">
							</div>
						{% endif %}
						<div class="blog-post-content">
							{{ blogPost.contenido }}
						</div>
					</div>
{% endblock %}